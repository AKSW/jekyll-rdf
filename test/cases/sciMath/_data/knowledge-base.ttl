@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix eg: <http://example.org/instance/> .

eg:math eg:mInteger "7"^^xsd:integer .
eg:math eg:mFloat "8.9"^^xsd:decimal .
eg:math eg:mDouble "4.2E9"^^xsd:double .
eg:math eg:mDoubleNE "2.0E-2"^^xsd:double .
eg:math eg:nInteger "-7"^^xsd:integer .
eg:math eg:nFloat "-8.9"^^xsd:decimal .
eg:math eg:nDouble "-4.2E9"^^xsd:double .
eg:math eg:nDoubleNE "-2.0E-2"^^xsd:double .

eg:math <http://example.org/int> "10"^^xsd:int .
eg:math <http://example.org/float> "7.25"^^xsd:float .
eg:math <http://example.org/negative> "-3"^^xsd:int .

eg:date <http://example.org/time> "12:45"^^xsd:time .
eg:date <http://example.org/date> "2018-06-12"^^xsd:date .
eg:date <http://example.org/dateTime> "2018-06-12T12:42+02:00"^^xsd:dateTime .
